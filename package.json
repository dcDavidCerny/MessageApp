{
  "name": "package",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "devOnlyVite": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "server:build": "tsc -p tsconfig.api.json",
    "server:start": "node api/dist/server.js",
    "server:watch": "nodemon --watch api -e ts --exec \"npm run server:build && npm run server:start\"",
    "dev": "concurrently -n \"API,React\" \"npm run server:watch\" \"npm run devOnlyVite\""
  },
  "dependencies": {
